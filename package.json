{
  "name": "digital-broker",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "postbuild": "npm run gen-icons && npm run gen-sw",
    "clean": "rimraf dist",
    "serve": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "gen-sw": "workbox generateSW workbox-config.cjs",
    "gen-icons": "pwa-asset-generator assets/logo.png dist/icons -i dist/index.html -m dist/manifest.json --maskable false"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.11",
    "@tsconfig/svelte": "^2.0.1",
    "autoprefixer": "^10.3.7",
    "cssnano": "^5.0.8",
    "gh-pages": "^3.2.3",
    "postcss": "^8.3.9",
    "postcss-load-config": "^3.1.0",
    "pwa-asset-generator": "^5.0.0",
    "rimraf": "^3.0.2",
    "svelte": "^3.37.0",
    "svelte-check": "^2.1.0",
    "svelte-preprocess": "^4.9.8",
    "tailwindcss": "^2.2.16",
    "tslib": "^2.2.0",
    "typescript": "^4.3.2",
    "vite": "^2.6.4",
    "workbox-cli": "^6.3.0"
  }
}
